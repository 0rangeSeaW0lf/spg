<section class="container agenda wrapper" id="agenda">
    <div class="row">
        <div class="col-xs-12 col-sm-3 pr-0 mb-1">
            <picture>
                <source srcset="https://res.cloudinary.com/dcxb6aehjk/image/upload/c_scale,q_auto,w_250/v1480763614/10_Day_Activities_osvfdo.png" media="(min-width: 768px)">
                <img src="https://res.cloudinary.com/dcxb6aehjk/image/upload/c_scale,q_auto,w_150/v1480763614/10_Day_Activities_osvfdo.png" class="img-fluid mx-auto d-block">
            </picture>
        </div>
        <div class="col-xs-12 col-sm-9">
            <div class="row">
                <div class="col-xs-12 text-xs-center text-md-left">
                    <h2 class="iese font-weight-bold mb-3">Spring Games by Day</h2>
                </div>
            </div>
            <%= render :partial => 'partials/agenda-beach' %>
            <%= render :partial => 'partials/agenda-sports' %>
            <%= render :partial => 'partials/agenda-spectators' %>
        </div>
    </div>
